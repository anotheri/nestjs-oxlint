{
  "name": "nestjs-oxlint",
  "version": "0.0.0",
  "type": "commonjs",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://github.com/anotheri/nestjs-oxlint.git"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --check",
    "format:fix": "prettier --write",
    "bake": "docker buildx bake -f docker-bake.hcl",
    "dev": "docker compose -f docker-compose.yml -f docker-compose.development.yml up -d",
    "down": "docker compose down --remove-orphans",
    "dev:exec": "docker exec -it nestjs-oxlint-api yarn",
    "clean": "rimraf dist",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug 0.0.0.0:9229 --watch --exec 'node --inspect-brk'",
    "start:prod": "node dist/main",
    "console": "node -r @swc-node/register -r ./bin/console.ts",
    "typecheck": "tsc --noEmit --incremental false",
    "lint": "oxlint --type-aware --max-warnings 0",
    "lint:fix": "yarn lint --fix",
    "test": "NODE_ENV=test vitest run --project unit",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage.enabled --coverage.provider=istanbul",
    "test:debug": "vitest --inspect-brk --inspect --logHeapUsage --threads=false",
    "test:e2e": "vitest run --project e2e",
    "db:env": "echo \"DATABASE_URL=${DATABASE_URL}\" >> prisma/.env && cat prisma/.env",
    "db:push": "prisma db push",
    "db:apply": "prisma db push --skip-generate",
    "db:seed": "prisma db seed",
    "db:studio": "prisma studio",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:pre": "prisma migrate dev --create-only",
    "db:migrate:reset": "prisma migrate reset",
    "db:migrate:status": "prisma migrate status",
    "db:migrate:deploy": "prisma migrate deploy",
    "postinstall": "[ -d '.husky' ] && husky install || true"
  },
  "dependencies": {
    "@faker-js/faker": "^9.9.0",
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/microservices": "^11.1.6",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/terminus": "^11.0.0",
    "@opentelemetry/api": "^1.9.0",
    "@prisma/client": "^6.15.0",
    "@prisma/instrumentation": "^6.15.0",
    "@sentry/nestjs": "^10.10.0",
    "@sentry/node": "^10.10.0",
    "@sentry/profiling-node": "^10.10.0",
    "@sinclair/typebox": "^0.34.41",
    "axios": "^1.11.0",
    "axios-retry": "^4.5.0",
    "class-validator": "^0.14.2",
    "crc": "^4.3.2",
    "dayjs": "^1.11.18",
    "json-schema-faker": "^0.5.9",
    "lodash": "^4.17.21",
    "modern-async": "^2.0.4",
    "nestjs-i18n": "^10.5.1",
    "nestjs-prisma": "^0.25.0",
    "nestjs-typebox": "^4.0.0",
    "pluralize-esm": "^9.0.5",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^11.0.7",
    "@nestjs/testing": "^11.1.6",
    "@swc-node/register": "^1.11.1",
    "@swc/cli": "0.7.8",
    "@swc/core": "^1.13.5",
    "@types/express": "^5.0.3",
    "@types/json-schema-faker": "^0.5.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/jwk-to-pem": "^2.0.3",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.3.1",
    "@types/passport": "^1.0.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-strategy": "^0.2.38",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-istanbul": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@yarnpkg/plugin-workspace-tools": "^4.1.6",
    "concurrently": "^9.2.1",
    "dotenv": "^17.2.2",
    "dotenv-expand": "^12.0.3",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.6",
    "msw": "^2.11.1",
    "oxlint": "^1.14.0",
    "oxlint-tsgolint": "^0.1.6",
    "prettier": "^3.6.2",
    "prettier-plugin-prisma": "^5.0.0",
    "prisma": "^6.15.0",
    "prisma-generator-fake-data": "^0.15.0",
    "prismabox": "^1.1.25",
    "rimraf": "^6.0.1",
    "supertest": "^7.1.4",
    "typescript": "^5.9.2",
    "unplugin-swc": "^1.5.7",
    "vitest": "^3.2.4",
    "vitest-mock-extended": "^3.1.0"
  },
  "resolutions": {
    "cross-spawn": "^7.0.6",
    "braces": "^3.0.3",
    "http-cache-semantics": "^4.1.1",
    "socks": "^2.8.3",
    "nanoid": "^3.3.8",
    "micromatch": "^4.0.8",
    "tar": "^6.2.1",
    "cookie": "0.7.2"
  },
  "packageManager": "yarn@4.9.4",
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
